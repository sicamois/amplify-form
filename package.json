{
  "name": "amplify-form",
  "description": "Create Form for your GaphQL API in Amplify",
  "author": "Pierre-olivier Sicamois",
  "keywords": [
    "react",
    "component",
    "form",
    "amplify",
    "aws-amplify",
    "graphQL",
    "graph-ql",
    "aws",
    "react-component",
    "ui",
    "typescript"
  ],
  "version": "1.1.0-beta.28",
  "main": "dist/bundle.js",
  "module": "dist/bundle.esm.js",
  "types": "dist/types.d.ts",
  "license": "MIT",
  "files": [
    "README.md",
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sicamois/amplify-form.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "fix-tags": "git fetch --tags -f",
    "format": "prettier --write .",
    "preci": "rm -fr node_modules",
    "ci": "yarn install --immutable --immutable-cache --check-cache",
    "test": "jest",
    "test-cov": "jest --coverage && coveralls < coverage/lcov.info",
    "test-build": "yarn build && yarn test-cov",
    "build": "rm -r -f dist && NODE_ENV=production yarn build:rollup",
    "build:tsdx": "tsdx build --format cjs,esm --tsconfig tsconfig.tsdx.json",
    "build:tsc": "tsc --project tsconfig.build.json && rm dist/tsconfig.build.tsbuildinfo",
    "build:tailwind": "tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles/tailwind.css --minify",
    "build:types": "tsc --project tsconfig.types.json && rm dist/tsconfig.types.tsbuildinfo",
    "build:rollup": "rollup --config",
    "publish-push": "npm publish && git push --quiet",
    "publish-beta": "yarn test-build && npm version prerelease --preid=beta &&  npm publish --tag=beta && git push --quiet",
    "publish-patch": "yarn test-build && npm version patch && yarn publish-push",
    "publish-minor": "yarn test-build && npm version minor && yarn publish-push",
    "publish-major": "yarn test-build && npm version major && yarn publish-push",
    "login-npm": "npm adduser",
    "prepare": "husky install"
  },
  "dependencies": {
    "chroma-js": "^2.1.2",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "react-dropzone": "^11.4.2",
    "react-select": "^5.2.1",
    "style-inject": "^0.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@optimize-lodash/rollup-plugin": "^3.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/bluebird": "^3.5.36",
    "@types/chroma-js": "^2.1.3",
    "@types/jest": "^27.0.3",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^17.0.1",
    "@types/react": "^17.0.37",
    "@types/react-test-renderer": "^17.0.1",
    "autoprefixer": "^10.4.0",
    "aws-amplify": "^4.3.11",
    "coveralls": "^3.1.1",
    "cssnano": "^5.0.14",
    "esbuild": "^0.14.8",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jest-dom": "latest",
    "eslint-plugin-testing-library": "latest",
    "husky": "latest",
    "jest": "^27.4.5",
    "lint-staged": "latest",
    "next": "^12.0.7",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.66.4",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.62.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-esbuild": "^4.8.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "tailwindcss": "^3.0.5",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "aws-amplify": ">= 4.3.0",
    "react": ">= 17",
    "react-dom": ">= 17"
  }
}
