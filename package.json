{
  "name": "amplify-form",
  "description": "Create Form for your GaphQL API in Amplify",
  "author": "Pierre-olivier Sicamois",
  "keywords": [
    "react",
    "component",
    "form",
    "amplify",
    "graphql",
    "graph-ql",
    "aws",
    "react-component",
    "ui"
  ],
  "version": "0.2.1-beta.36",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=15",
    "npm": ">=5"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sicamois/amplify-form.git"
  },
  "scripts": {
    "build": "rm -rf lib && rollup -c",
    "start": "rm -rf lib && rollup -c -w",
    "prettify": "prettier --write src",
    "build:ts": "NODE_ENV=production tsc -p .",
    "build:webpack": "NODE_ENV=production webpack",
    "build:tailwind": "NODE_ENV=production postcss ./src/styles/index.css -o ./src/styles/tailwind.css",
    "version-beta": "yarn build && npm version prerelease --preid=beta",
    "version-minor": "yarn build && npm version minor"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "react-dropzone": "^11.4.2",
    "react-select": "^5.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "autoprefixer": "^10.4.0",
    "aws-amplify": "^4.3.10",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "postcss": "^8.4.4",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.60.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "tailwindcss": "^2.2.19",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "aws-amplify": "^4.3.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  }
}
