{
  "name": "amplify-form",
  "description": "Create Form for your GaphQL API in Amplify",
  "author": "Pierre-olivier Sicamois",
  "keywords": [
    "react",
    "component",
    "form",
    "amplify",
    "graphql",
    "graph-ql",
    "aws",
    "react-component",
    "ui"
  ],
  "version": "0.2.1-beta.66",
  "main": "dist/index.js",
  "typings": "dist/types.d.ts",
  "license": "MIT",
  "files": [
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sicamois/amplify-form.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "build": "rm -r -f dist && yarn build:tsdx",
    "build:tsdx": "tsdx build --name amplify-form --format cjs,esm,umd --tsconfig ./tsconfig.tsdx.json",
    "version-beta": "yarn build && npm version prerelease --preid=beta",
    "version-minor": "yarn build && npm version minor"
  },
  "dependencies": {
    "formik": "^2.2.9",
    "loadash": "^1.0.0",
    "react-dropzone": "^11.4.2",
    "react-select": "^5.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "autoprefixer": "^10.4.0",
    "aws-amplify": "^4.3.10",
    "cssnano": "^5.0.12",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "next": "12.0.7",
    "postcss": "^8.4.4",
    "postcss-import": "^14.0.2",
    "prismjs": "^1.25.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^2.2.19",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "4.5.2"
  },
  "peerDependencies": {
    "aws-amplify": "^4.3",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  }
}
