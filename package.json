{
  "name": "amplify-form",
  "description": "Create Form for your GaphQL API in Amplify",
  "author": "Pierre-olivier Sicamois",
  "keywords": [
    "react",
    "component",
    "form",
    "amplify",
    "graphql",
    "graph-ql",
    "aws",
    "react-component",
    "ui"
  ],
  "version": "0.2.1-beta.22",
  "private": false,
  "license": "MIT",
  "main": "./lib/AmplifyForm.js",
  "files": [
    "lib",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sicamois/amplify-form.git"
  },
  "scripts": {
    "start": "PORT=8080 react-scripts --openssl-legacy-provider start",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check -- --watch",
    "prettify": "prettier --write src",
    "prettier:check": "prettier --check src",
    "build": "rm -rf lib && yarn run build:webpack",
    "build:ts": "NODE_ENV=production tsc -p .",
    "build:webpack": "NODE_ENV=production webpack",
    "build:tailwind": "NODE_ENV=production postcss ./src/index.css -o ./lib/index.css",
    "version-beta": "yarn build && npm version prerelease --preid=beta",
    "version-minor": "yarn build && npm version minor"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "react-dropzone": "^11.4.2",
    "react-select": "^5.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/preset-typescript": "^7.16.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.0.1",
    "prettier": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^2.2.19",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "aws-amplify": "^4.3.10"
  },
  "engines": {
    "node": ">=15"
  }
}
