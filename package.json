{
  "name": "amplify-form",
  "description": "Create Form for your GaphQL API in Amplify",
  "author": "Pierre-olivier Sicamois",
  "keywords": [
    "react",
    "component",
    "form",
    "amplify",
    "aws-amplify",
    "graphQL",
    "graph-ql",
    "aws",
    "react-component",
    "ui",
    "typescript"
  ],
  "version": "1.0.2-beta.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/amplify-form.esm.js",
  "license": "MIT",
  "files": [
    "README.md",
    "dist"
  ],
  "sideEffects": true,
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sicamois/amplify-form.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "build": "rm -r -f dist && yarn build:tsdx",
    "build:tsdx": "tsdx build --tsconfig tsconfig.tsdx.json",
    "publish-beta": "yarn build && npm version prerelease --preid=beta && npm publish -tag=beta && git push",
    "publish-patch": "yarn build && npm version patch && npm publish && git push",
    "publish-minor": "yarn build && npm version minor && npm publish && git push",
    "publish-major": "yarn build && npm version major && npm publish && git push",
    "login-npm": "npm adduser"
  },
  "dependencies": {
    "formik": "^2.2.9",
    "loadash": "^1.0.0",
    "react-dropzone": "^11.4.2",
    "react-select": "^5.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "16.11.13",
    "@types/react": "17.0.37",
    "autoprefixer": "^10.4.0",
    "aws-amplify": "^4.3.10",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "next": "12.0.7",
    "node-notifier": "^10.0.0",
    "postcss": "^8.4.4",
    "prettier": "^2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "tailwindcss": "^3.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "aws-amplify": "^4.3",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  }
}
