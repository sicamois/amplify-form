{
  "name": "amplify-form",
  "description": "Create Form for your GaphQL API in Amplify",
  "author": "Pierre-olivier Sicamois",
  "keywords": [
    "react",
    "component",
    "form",
    "amplify",
    "graphql",
    "graph-ql",
    "aws",
    "react-component",
    "ui"
  ],
  "version": "0.1.0-beta.24",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sicamois/amplify-form.git"
  },
  "scripts": {
    "start": "PORT=8080 react-scripts --openssl-legacy-provider start",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check -- --watch",
    "prettify": "prettier --write src",
    "prettier:check": "prettier --check src",
    "build": "rm -rf dist && yarn run build:types && yarn run build:js",
    "build:types": "NODE_ENV=production tsc --emitDeclarationOnly",
    "build:js": "NODE_ENV=production rollup -c",
    "version-beta": "yarn build && npm version prerelease --preid=beta"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "aws-amplify": "^4.3.8",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-select": "^5.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.4",
    "prettier": "^2.5.0",
    "rollup": "^2.60.2",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^4.1.2",
    "tailwindcss": "^2.2.19"
  },
  "peerDependencies": {
    "aws-amplify": "^4.3.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
