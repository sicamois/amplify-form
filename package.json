{
  "name": "amplify-form",
  "description": "Create Form for your GaphQL API in Amplify",
  "author": "Pierre-olivier Sicamois",
  "keywords": [
    "react",
    "component",
    "form",
    "amplify",
    "aws-amplify",
    "graphQL",
    "graph-ql",
    "aws",
    "react-component",
    "ui",
    "typescript"
  ],
  "version": "1.1.0-beta.30",
  "main": "dist/bundle.js",
  "module": "dist/bundle.esm.js",
  "types": "dist/types.d.ts",
  "license": "MIT",
  "files": [
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sicamois/amplify-form.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "fix-tags": "git fetch --tags -f",
    "format": "prettier --write .",
    "ci": "yarn install --immutable",
    "test": "jest",
    "test-cov": "yarn test --coverage && coveralls < coverage/lcov.info",
    "test-build": "yarn build && yarn test-cov",
    "build": "rm -r -f dist && NODE_ENV=production yarn build:rollup",
    "build:rollup": "rollup --config"
  },
  "packageManager": "yarn@3.1.1",
  "dependencies": {
    "chroma-js": "^2.1.2",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "react-dropzone": "^12.0.4",
    "react-select": "^5.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@rollup/plugin-typescript": "^8.3.0",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/chroma-js": "^2.1.3",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^18.11.6",
    "@types/react": "^17.0.38",
    "@types/react-test-renderer": "^17.0.1",
    "@types/testing-library__jest-dom": "^5.14.2",
    "autoprefixer": "^10.4.1",
    "aws-amplify": "^4.3.11",
    "coveralls": "^3.1.1",
    "cssnano": "^5.0.14",
    "eslint": "^8.5.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-testing-library": "^5.0.1",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.5",
    "next": "^12.0.7",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.67.1",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.62.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "tailwindcss": "^3.0.9",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "aws-amplify": ">=4.3",
    "react": ">=17",
    "react-dom": ">=17"
  }
}
