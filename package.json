{
  "name": "amplify-form",
  "description": "Create Form for your GaphQL API in Amplify",
  "author": "Pierre-olivier Sicamois",
  "keywords": [
    "react",
    "component",
    "form",
    "amplify",
    "aws-amplify",
    "graphQL",
    "graph-ql",
    "aws",
    "react-component",
    "ui",
    "typescript"
  ],
  "version": "1.0.4-beta.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/amplify-form.esm.js",
  "license": "MIT",
  "files": [
    "README.md",
    "dist"
  ],
  "sideEffects": true,
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sicamois/amplify-form.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "fix-tags": "git fetch --tags -f",
    "prettier-fix": "prettier --write .",
    "build": "rm -r -f dist && yarn build:tsdx",
    "build:tsdx": "tsdx build --tsconfig tsconfig.tsdx.json",
    "publish-push": "npm publish && git push --quiet",
    "publish-patch": "yarn build && npm version patch && yarn publish-push",
    "publish-minor": "yarn build && npm version minor && yarn publish-push",
    "publish-major": "yarn build && npm version major && yarn publish-push",
    "login-npm": "npm adduser",
    "prepare": "husky install"
  },
  "dependencies": {
    "formik": "^2.2.9",
    "loadash": "^1.0.0",
    "react-dropzone": "^11.4.2",
    "react-select": "^5.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.0",
    "@types/react": "^17.0.37",
    "autoprefixer": "^10.4.0",
    "aws-amplify": "^4.3.11",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "next": "^12.0.7",
    "node-notifier": "^10.0.0",
    "postcss": "^8.4.5",
    "prettier": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "tailwindcss": "^3.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "aws-amplify": "^4.3.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
